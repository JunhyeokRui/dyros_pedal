<launch>
 <!-- pedal node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="joystick" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="autorepeat_rate" value="30" />
    <param name="deadzone" value="0.20" />
    <remap from="/joy" to="/joy_wh"/>
  </node>

</launch>
